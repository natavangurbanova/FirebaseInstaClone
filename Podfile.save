platform :ios, '17.2'

target 'FirebaseInstaClone' do
    # Comment the next line if you don't want to use dynamic frameworks
    use_frameworks!
    pod 'SnapKit'
    
    # Pods for FirebaseInstaClone
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Storage'
    pod 'SDWebImage'
end
